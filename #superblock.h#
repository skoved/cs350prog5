#ifndef SUPERBLOCK_H
#define SUPERBLOCK_H

using namespace std;
/**
 * Super block struct
 */

struct superblock{

    int blockSize;
    int numBlocks;
    int inodeMap;
    int freeBlockList;  
    int freeBlockList;//Current instruction pointer [][][][][][x][][][]
  unsigned int current_instr;
};

#endif
